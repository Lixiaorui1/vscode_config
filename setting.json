{
  /*
   * vscode基础配置
   */
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  /*
   * eslint和 prettier配置
   */
  "eslint.validate": ["javascript", "javascriptreact", "vue", "html"],
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 120
}
